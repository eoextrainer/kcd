# API Configuration
API_V1_STR=/api/v1
PROJECT_NAME=KCD API

# Frontend
FRONTEND_URL=http://localhost:3000
CORS_ORIGINS=http://localhost:3000,http://localhost:5173

# Environment
ENVIRONMENT=development

# Feature Flags
ENABLE_GUEST_ACCESS=true
